{
  "version": 2,
  "builds": [
    { "src": "**/*", "use": "@vercel/static" }
  ],
  "rewrites": [
    {
      "source": "/startup/(.*)",
      "destination": "/index.html"
    },
    {
      "source": "/featured",
      "destination": "/featured.html"
    },
    {
      "source": "/dashboard",
      "destination": "/dashboard.html"
    },
    {
      "source": "/success",
      "destination": "/success.html"
    },
    {
      "source": "/((?!api|_next|_static|favicon.ico|src|auth).*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}
